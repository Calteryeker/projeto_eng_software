<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.UsuarioMainPageController">
  <children>
    <VBox layoutY="87.0" prefHeight="200.0" prefWidth="100.0" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button fx:id="backBt" mnemonicParsing="false" onAction="#onBackBtAction" text="←" />
        <HBox alignment="CENTER">
          <children>
            <Text fx:id="welcomeTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Bem vindo {name}">
              <font>
                <Font size="18.0" />
              </font>
            </Text>
          </children>
        </HBox>
        <VBox prefHeight="296.0" prefWidth="575.0" spacing="10.0">
          <children>
            <Button fx:id="startShoppingBt" mnemonicParsing="false" onAction="#onStartShoppingBtAction" text="Administrar despesas" />
            <Button fx:id="cureServiceBt" mnemonicParsing="false" onAction="#onCureServiceBtAction" text="Administrar metas" />
          </children>
        </VBox>
      </children>
      <padding>
        <Insets left="25.0" top="25.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
